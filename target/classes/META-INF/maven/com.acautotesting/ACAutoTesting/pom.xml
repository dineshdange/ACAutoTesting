<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.acautotesting</groupId>
  <artifactId>ACAutoTesting</artifactId>
  <version>0.0.1-SNAPSHOT</version>
  <dependencies>
  <dependency>
			<groupId>com.selenium</groupId>
			<artifactId>byte-buddy</artifactId>
			<version>1.8.15</version>
			<scope>system</scope>
			<systemPath>${basedir}/lib/byte-buddy-1.8.15.jar</systemPath>
	</dependency>
	<dependency>
			<groupId>com.selenium</groupId>
			<artifactId>client-combined</artifactId>
			<version>3.141.59</version>
			<scope>system</scope>
			<systemPath>${basedir}/lib/client-combined-3.141.59-sources.jar</systemPath>
	</dependency>
	<dependency>
			<groupId>com.selenium</groupId>
			<artifactId>client-combined1</artifactId>
			<version>3.141.59</version>
			<scope>system</scope>
			<systemPath>${basedir}/lib/client-combined-3.141.59.jar</systemPath>
	</dependency>
	<dependency>
			<groupId>com.selenium</groupId>
			<artifactId>commons-exec</artifactId>
			<version>1.3</version>
			<scope>system</scope>
			<systemPath>${basedir}/lib/commons-exec-1.3.jar</systemPath>
	</dependency>
	<dependency>
			<groupId>com.selenium</groupId>
			<artifactId>guava</artifactId>
			<version>25.0</version>
			<scope>system</scope>
			<systemPath>${basedir}/lib/guava-25.0-jre.jar</systemPath>
	</dependency>
	<dependency>
			<groupId>com.selenium</groupId>
			<artifactId>okhttp</artifactId>
			<version>3.11.0</version>
			<scope>system</scope>
			<systemPath>${basedir}/lib/okhttp-3.11.0.jar</systemPath>
	</dependency>
	<dependency>
			<groupId>com.selenium</groupId>
			<artifactId>okio</artifactId>
			<version>1.14.0</version>
			<scope>system</scope>
			<systemPath>${basedir}/lib/okio-1.14.0.jar</systemPath>
	</dependency>
	<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
			<version>4.12</version>
			<scope>test</scope>
	</dependency> 
	</dependencies> 
  <build>
    <sourceDirectory>src</sourceDirectory>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.6.1</version>
        <configuration>
          <source>1.8</source>
          <target>1.8</target>
        </configuration>
      </plugin>
    </plugins>
  </build>
</project>